@model List<GeoTag>
@{
    var geoTagDictionary = Model.ToDictionary(g => g.Title, g => g);
}

<div class="background-home">
    <a href="@Url.Action("Detail", "Home", new {id = geoTagDictionary["Problems"].Id})">
        <div id="image-container1">
            <p class="image-text">Problems</p>
            <img src="~/images/geotags/geotag-black.png" />
        </div>
    </a>
    <a href="@Url.Action("Detail", "Home", new {id = geoTagDictionary["A Solution"].Id})">
        <div id="image-container2">
            <p class="image-text">A Solution</p>
            <img src="~/images/geotags/geotag-black.png" />
        </div>
    </a>
    <a href="@Url.Action("Detail", "Home", new {id = geoTagDictionary["Ignore This"].Id})">
        <div id="image-container3">
            <p class="image-text">Ignore This</p>
            <img src="~/images/geotags/geotag-black.png" />
        </div>
    </a>
    <a href="@Url.Action("Detail", "Home", new {id = geoTagDictionary["Deleuzian Ontology in Education"].Id})">
        <div id="image-container4">
            <img src="~/images/geotags/geotag-white.png" />
            <p class="image-text">Deleuzian Ontology <br/> in Education</p>
        </div>
    </a>
    <a href="@Url.Action("Detail", "Home", new {id = geoTagDictionary["An Inquiry-Machine"].Id})">
        <div id="image-container5">
            <img src="~/images/geotags/geotag-white.png" />
            <p class="image-text">An Inquiry-Machine</p>
        </div>
    </a>
    <a href="@Url.Action("MolarMosaics", "Home")">
        <div id="image-container6">
            <img src="~/images/geotags/geotag-white.png" />
            <p class="image-text">Molar Mosaics</p>
        </div>
        <img id="yellow-mosaic1" src="~/images/mosaics/yellow-mosaic.png" />
        <img id="yellow-mosaic2" src="~/images/mosaics/yellow-mosaic.png" />
        <img id="yellow-mosaic3" src="~/images/mosaics/yellow-mosaic.png" />
    </a>
    <a href="@Url.Action("MolecularMosaics", "Home")">
        <div id="image-container7">
            <img src="~/images/geotags/geotag-white.png"/>
            <p class="image-text">Molecular Mosaics</p>
        </div>
        <img id="blue-mosaic1" src="~/images/mosaics/mosaic14.png" />
        <img id="blue-mosaic2" src="~/images/mosaics/mosaic14.png" />
        <img id="blue-mosaic3" src="~/images/mosaics/mosaic14.png" />
        <img id="blue-mosaic4" src="~/images/mosaics/mosaic14.png" />
        <img id="blue-mosaic5" src="~/images/mosaics/mosaic14.png" />
    </a>
    <a href="@Url.Action("Kaleidoscoping", "Home")">
        <div id="image-container8">
            <img src="~/images/geotags/geotag-white.png" />
            <p class="image-text">Kaleido-scoping</p>
        </div>
    </a>
</div>
