@model List<GeoTag>

@{
    ViewData["Title"] = "Home Page";
}

<div class="background-home">
    @{
        var geoTagDictionary = Model.ToDictionary(g => g.Title, g => g);
    }
    <a href="Mosaics/@geoTagDictionary["Problems"]?.Id">
        <div id="image-container1">
            <p class="image-text">Problems</p>
            <img src="~/images/geotags/geotag-black.png" />
        </div>
        <img id="blue-mosaic1" src="~/images/mosaics/mosaic14.png" />
    </a>
    <a href="Mosaics/@geoTagDictionary["A Solution"]?.Id">
        <div id="image-container2">
